var tms1 = new Cesium.UrlTemplateImageryProvider({
       url: 'http://localhost:8080/{z}/{x}/{reverseY}.png',
       tilingScheme: new Cesium.GeographicTilingScheme(),
       minimumLevel: 0,
       maximumLevel: 4,
       tileWidth: 256,
       tileHeight: 256
    });


var tms2 = new Cesium.TileMapServiceImageryProvider({
   url : 'http://localhost:8080'
});


var viewer = new Cesium.Viewer('cesiumContainer');
const layer = viewer.scene.imageryLayers.addImageryProvider(tms2);
layer.minificationFilter = Cesium.TextureMinificationFilter.NEAREST;
layer.magnificationFilter = Cesium.TextureMagnificationFilter.NEAREST;
layer.alpha = 0.75;


