{
  "include": [
    "src/electron/*"
  ],
  "compilerOptions": {
    "outDir": "./dist-src-electron/",
    "strict": true,
    "noImplicitAny": true,
    "sourceMap": true,
    "module": "ES2020",
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "moduleResolution": "node",
    "target": "ES2020",
    "skipLibCheck": true
  }
}