.storiesSelector
  position: relative
  width: 100%
  height: 100%
  background-color: #10161A
  pointer-events: auto

.header
  display: flex
  justify-content: space-between
  align-items: center
  padding: 10px
  background-color: #182026

.title
  color: #fff
  text-align: center
  text-transform: uppercase
  font-size: 1.1em

.backButton, .filter
  color: #fff
  font-size: 1.1em

.filter
  color: rgba(255, 255, 255, 0.24)
