@require '../../../variables.styl'

@font-face
  font-style: normal
  font-family: NotesEsa
  src: url('../../../fonts/NotesEsaReg.otf')

:global(html), :global(body), :global(#app)
  margin: 0
  height: 100%

.app
  position: relative
  width: 100%
  height: 100%
  background-color: $black
  font-family: Arial, Helvetica, sans-serif

.logo
  position: absolute
  top: emCalc(22px)
  left: emCalc(22px)
  z-index: 1
  fill: $textWhite
