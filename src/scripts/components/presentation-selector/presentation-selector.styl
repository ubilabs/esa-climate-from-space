.presentationSelector
  position: relative
  width: 100%
  height: 100%
  background-color: #10161A
  pointer-events: auto

.header
  display: flex
  justify-content: space-between
  padding: 10px
  background-color: #182026

.title
  flex-grow: 1
  padding-right: 115px
  color: #fff
  text-align: center
  text-transform: uppercase
  font-size: 2em

.backButton
  margin: 10px 0 10px 0
  padding: 10px
  color: #fff
  font-size: 2em
