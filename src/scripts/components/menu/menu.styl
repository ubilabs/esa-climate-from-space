.menuContainer
    height: 30px
    width: 30px

.menuButton
    border-radius: 50%
    background-color: #ccc
    font-size: 1em
    outline: 0

.menuList
    min-width: 250px
    background-color: #fff;
    padding: 0

.menuListItem
    padding: 10px 15px
    margin: 7px

.menuListItem:hover,
.menuListItem:focus
    background-color: #e0f1ff
